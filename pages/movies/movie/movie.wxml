
<import src="../stars/stars.wxml"/>
<template name="movieTemplate" >
  <view class='movieContainer' catchtap='movielist' data-movieId='{{movieId}}' hover-class='hover-class'>
    <image  class="movieimage" src="{{coverageUrl}}"></image>
    <text class='movietext'>{{title}}</text>
    <template is="starsTemplate" data="{{stars:stars ,score:average}}"/>
  </view>
</template>